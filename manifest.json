{
  "name": "niichrome 2chブラウザ",
  "short_name": "niichrome",
  "description": "Chromeアプリ版２ちゃんねるブラウザ",
  "version": "1.1.3",
  "manifest_version": 2,
  "minimum_chrome_version": "37",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "icons": {
    "128": "128.png"
  },
  "permissions": [
    "contextMenus",
    "webview",
    "<all_urls>",
    "fullscreen",
    "storage",
    "unlimitedStorage"
  ]
}